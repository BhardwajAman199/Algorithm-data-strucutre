#include <iostream> 
using namespace std; 
class marks{ 
    int intmarks; 
    int extmars; 
    public : 
     marks(){ 
         intmarks = 0; 
         extmars = 0; 
     } 
      marks(int in, int ex){ 
         intmarks = in; 
         extmars = ex; 
     } 
     void display(){ 
         cout<<intmarks<<endl<<extmars<<endl; 
     } 
     marks operator + (marks m){              // syntax , it returntype as class  
         marks temp ;                           //  temp is object here  
         temp.intmarks = intmarks + m.intmarks; 
         temp.extmars = extmars + m.extmars; 
         return temp; 
     } 
}; 
int main() 
{ 
    marks m1(10,20),m2(30,40); 
    marks m3 = m1 + m2; 
    m3.display(); 
} 
