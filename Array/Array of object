#include <iostream>  
using namespace std;  
class person   
{  
   int age;  
   char name[10];  
   public :  
     void read()  
      {  
          cout<<"enter the name ";  
          cin>>name;  
         cout<<"enter the age ";  
          cin>>age;  
      }  
     void display()  
       {  
           cout<<"name "<<name<<endl;  
           cout<<"Age ="<<age<<endl;  

        }  
};  
int main()  
{  
    int size,i;  
    person p[10];     // array of 10 object  
    cin>>size;  
    for(i =0;i<size;i++)  
     {
           p[i].read() 
           p[i].display();  
     }  
     return 0;  
    
} 
