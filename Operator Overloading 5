#include <iostream> 
using namespace std; 
class Marks 
{ 
    int subject[3]; 
    public : 
     Marks(int sub1, int sub2, int sub3){ 
         subject[0] = sub1; 
         subject[1] = sub2; 
         subject[2] = sub3; 
     } 
     int operator[](int p) 
     { 
         return subject[p]; 
     } 
}; 
int main() 
{ 
    Marks aman(22,55,77); 
    cout<<aman[0]<<endl; 
    cout<<aman[1]<<endl; 
    cout<<aman[2]<<endl; 
    return 0; 

} 
