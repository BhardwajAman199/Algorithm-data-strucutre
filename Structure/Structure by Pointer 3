#include <iostream> 
using namespace std; 
struct student { 
    int rollno; 
    char sex; 
}; 
int main() 
{ 
   student anil; 
   student *anilptr;             // intialization of pointer 
   anilptr = &anil; 
   anilptr->rollno = 1234; 
   anilptr->sex = 'm'; 
   cout<<anilptr->rollno<<endl; 
   cout<<anilptr->sex<<endl; 
  return 0; 
} 
