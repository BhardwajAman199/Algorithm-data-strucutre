#include <iostream> 
using namespace std; 
struct student { 
    int rollno; 
    char sex; 
}; 
int main() 

{ 
   student anil; 
   student *anilptr;             // intialization of pointer 
   anilptr = &anil; 
   anil.rollno = 1234; 
   anilptr->sex = 'm'; 
   cout<<anil.rollno<<endl; 
   cout<<anil.sex<<endl; 
   return 0; 

} 
