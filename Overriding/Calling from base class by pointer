#include <iostream> 
#include <string> 
using namespace std; 
class person{ 
    public : 
    void introduce(){ 
        cout<<"hey from student "<<endl; 
    } 
}; 
class student : public person { 
    public : 
     void introduce(){ 
         cout<<" hey from person "<<endl; 
     } 
}; 
void WhosThis(person *p){ 
    p->introduce(); 
} 
int main() 
{ 
   student aman; 
   aman.introduce(); 
   WhosThis(&aman); 
   return 0; 
} 
