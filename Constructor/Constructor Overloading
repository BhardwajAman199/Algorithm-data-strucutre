#include <iostream> 
using namespace std; 
class Fourside 
{ 
    float area; 
    public : 
    Fourside(int side) 
    { 
        area = side*side; 
        cout<<"Area of the square "<<area<<endl; 
    } 
    Fourside(int side1,int side2) 
     { 
         area = side1*side2; 
         cout<<" Area of the Rectangle : "<<area<<endl; 
     } 
}; 
int main() 
{ 
    Fourside square(2),Rectangle(5,2); 
     return 0; 
} 
