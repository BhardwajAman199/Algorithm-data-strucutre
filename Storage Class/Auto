#include <iostream> 
using namespace std; 
void fun1() 
 { 
     int a=10; 
     cout<<"\n a =" <<a;   // auto integer local to fun1() 
} 
 void fun2() 
{ 
    int a = 20; 
    cout<<"\n a =" <<a ;  // auto integer local to fuc2() 
} 
int main() 
{ 
    int a=30;         // auto integer local to main() 
    fun1(); 
    fun2(); 
    cout<<"\n a ="<<a; 
    return 0; 
} 
