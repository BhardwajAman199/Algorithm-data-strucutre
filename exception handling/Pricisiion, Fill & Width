#include <iostream> 
using namespace std; 
int main() 
{ 
      cout.precision(4); 
      cout<< 123.556<< endl; 
      cout.width(10); 
      cout<<"hi"<<endl; 
      cout.width(10); 
       cout<<123.556<<endl; 

  cout.fill('*'); 
  cout.precision(4); 
  cout<< 123.556<< endl; 
  cout.width(10); 
  cout<<"hi"<<endl; 
  cout.width(10); 
  cout<<123.556<<endl; 

       cout.setf(ios :: left); 
       cout.width(10); 
       cout<<"hi"<<endl;
       cout.width(10); 
       cout<<123.556<<endl; 
return 0; 
}
