#include <iostream> 
using namespace std; 
class Person{ 
    public : 
      void introduce(){ 
          cout<<" hi i am a person "<<endl; 
      } 
}; 
class Student  : public Person { 
    public : 
       void introduce(){ 
        cout<<" hi i am a person and i am awesome  "<<endl;   
       } 
}; 
int main() 
{ 
    Student obj; 
    obj.introduce(); 
    return 0; 
} 
